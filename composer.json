{
    "name": "lav45/yii2-project-configuration",
    "description": "This extension helps you to easily store and retrieve settings for your project.",
    "keywords": [
        "yii2",
        "extension",
        "settings",
        "config"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "homepage": "https://github.com/lav45/yii2-project-configuration",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Aleksey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "yiisoft/yii2": "2.0.*",
        "lav45/yii2-settings": ">=1.2.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "files": [
            "src/config.php"
        ],
        "psr-4": {
            "lav45\\projectConfiguration\\console\\controllers\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
